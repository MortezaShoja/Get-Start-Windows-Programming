<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsP" targetNamespace="http://www.tempuri.org/dsP.xsd" xmlns:mstns="http://www.tempuri.org/dsP.xsd" xmlns="http://www.tempuri.org/dsP.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsP" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="phones">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Family" type="xs:string" />
              <xs:element name="Name" type="xs:string" />
              <xs:element name="Phone" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:phones" />
      <xs:field xpath="mstns:Family" />
      <xs:field xpath="mstns:Name" />
    </xs:unique>
  </xs:element>
</xs:schema>