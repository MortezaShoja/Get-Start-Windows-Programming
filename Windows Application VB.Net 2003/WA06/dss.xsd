<?xml version="1.0" standalone="yes"?>
<xs:schema id="dss" targetNamespace="http://www.tempuri.org/dss.xsd" xmlns:mstns="http://www.tempuri.org/dss.xsd" xmlns="http://www.tempuri.org/dss.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dss" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="employee">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="emp_id" type="xs:string" />
              <xs:element name="fname" type="xs:string" />
              <xs:element name="lname" type="xs:string" />
              <xs:element name="job_id" type="xs:short" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:employee" />
      <xs:field xpath="mstns:emp_id" />
    </xs:unique>
  </xs:element>
</xs:schema>